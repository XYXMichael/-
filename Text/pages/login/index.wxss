page{
  background: rgb(243, 231, 216);
}
.login {
  padding: 64rpx;
  margin-top: 50rpx;
}
.header{
  margin-top: 160rpx;
  display: flex;
  justify-content: center;
}
.header image{
  width: 100%;
  height: 300rpx;
  opacity: 1;
}
.form-item image{
  width: 32rpx;
  height: 32rpx;
  margin-right: 32rpx;
  opacity: 1;
  background: rgba(216, 216, 216, 0.01);
}
.form-item{
  height: 98rpx;
  border-radius: 49rpx;
  opacity: 1;
  background: #FFFFFF;
  /* border: 2px dashed rgba(232, 232, 232, 0.86); */
  box-shadow: 0px 0px 4px -8px rgba(0, 0, 0, 0.1);
  display: flex;
  align-items: center;
  padding: 0 30rpx;
  margin-bottom: 48rpx;
}
.checkbox-item {
  margin-left: 43rpx;
  display: flex;
  align-items: center;
}
button {
  margin-top: 50rpx;
  width: 614rpx;
  height: 96rpx;
  border-radius: 48rpx;
  opacity: 1;
  background: linear-gradient(90deg, rgba(193,239,245,0.90) 0%, #0AE8F0 0%, #88F2DF 100%);
  box-shadow: 0px 0px 12px 0px rgba(0, 0, 0, 0.1);
}